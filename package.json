{
  "name": "myhome",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "build:ts": "tsc",
    "build:code": "cd ./src/elm-front; elm-graphql --schema ../../dist/schema.json --endpoint dummy",
    "build:schema": "apollo-codegen introspect-schema ./src/backend/graphql/schema.graphql --output ./dist/schema.json",
    "build:copy": "copyfiles -u 1 src/**/*.graphql dist/",
    "build": "npm run build:ts && npm run build:copy && npm run webpack",
    "start": "node ./dist/backend/server.js",
    "start:front": "cd ./src/elm-front; elm-app start;",
    "start:prod": "npm run migrate && npm start",
    "migrate": "migrate -c ./dist",
    "webpack": "webpack --config ./src/webpack.config.js"
  },
  "dependencies": {
    "@influential/elm-graphql": "git+https://github.com/Bellk17/elm-graphql.git",
    "@types/cors": "^2.8.4",
    "@types/graphql": "^0.13.1",
    "@types/koa": "^2.0.39",
    "@types/koa-bodyparser": "^3.0.23",
    "@types/koa-router": "^7.0.22",
    "@types/material-ui": "^0.18.3",
    "@types/moment": "^2.13.0",
    "@types/pg": "^6.1.43",
    "@types/ramda": "^0.25.0",
    "@types/react": "^16.0.18",
    "@types/react-chartjs-2": "^2.0.2",
    "@types/react-dom": "^16.0.2",
    "@types/react-router": "^4.0.14",
    "@types/webpack": "^3.0.14",
    "apollo-client": "^1.9.1",
    "apollo-codegen": "^0.19.1",
    "apollo-react": "^0.7.1",
    "apollo-server-koa": "^1.0.5",
    "awesome-typescript-loader": "^3.1.3",
    "chart.js": "^2.7.1",
    "cors": "^2.8.4",
    "elm-graphql": "https://github.com/Bellk17/elm-graphql",
    "graphql": "^0.10.5",
    "graphql-server-koa": "^1.2.0",
    "graphql-tag": "^2.4.2",
    "graphql-tools": "^1.1.0",
    "koa": "^2.3.0",
    "koa-bodyparser": "^4.2.0",
    "koa-router": "^7.2.1",
    "koa2-cors": "^2.0.5",
    "material-ui": "^0.18.7",
    "migrate": "^0.2.3",
    "moment": "^2.19.1",
    "pg": "^7.1.0",
    "pg-camelcase": "0.0.3",
    "ramda": "^0.25.0",
    "react": "^16.0.0",
    "react-apollo": "^1.4.12",
    "react-chartjs-2": "^2.6.4",
    "react-dom": "^16.0.0",
    "react-router": "^4.1.2",
    "react-tap-event-plugin": "^2.0.1",
    "rxjs-es": "^5.0.0-beta.12",
    "source-map-loader": "^0.2.1",
    "tslint": "^5.5.0",
    "typescript": "^2.4.2",
    "webpack": "^3.5.3"
  },
  "devDependencies": {
    "@types/chart.js": "^2.7.18",
    "@types/koa2-cors": "^2.0.1",
    "copyfiles": "^2.0.0",
    "koa-webpack-middleware": "^1.0.7"
  }
}
